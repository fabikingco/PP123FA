<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fabik.parkingapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:name=".appInit"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ConfigMondedaActivity"></activity>
        <activity android:name=".TiposDeVehiculosActivity" />
        <activity android:name=".SearchActivity" />
        <activity android:name=".ConfiguracionActivity" />

        <uses-library
            android:name="com.pos.device"
            android:required="false" />

        <activity android:name=".ListadoMain">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".IngresarVehiculos" />
        <activity android:name=".SacarVehiculos" />
        <activity android:name=".ListarVehiculos" />
        <activity android:name=".ListadoTotal" />
        <activity android:name=".MenuPrincipal" />
    </application>

</manifest>